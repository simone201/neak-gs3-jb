#!/sbin/busybox sh

export _PATH="$PATH"
export PATH=/sbin

busybox cd /
busybox rm /init

if busybox grep -q 1 /sys/class/power_supply/battery/batt_lp_charging ; then
  # low power mode
  /sbin/busybox cp -f lpm.rc init.rc
  /sbin/busybox rm -f init.smdk4x12.rc
fi

busybox mv -f /init-cyano /init

export PATH="${_PATH}"
exec /init
